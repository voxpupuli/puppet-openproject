{
  "name": "puppet-openproject",
  "version": "1.0.1",
  "author": "Vox Pupuli",
  "summary": "A puppet module to install and configure OpenProject",
  "license": "GPL-3.0-or-later",
  "source": "https://github.com/voxpupuli/puppet-openproject",
  "project_page": "https://github.com/voxpupuli/puppet-openproject",
  "issues_url": "https://github.com/voxpupuli/puppet-openproject/issues",
  "dependencies": [
    {
      "name": "puppetlabs/apt",
      "version_requirement": ">= 7.0.0 < 10.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 5.0.0 < 10.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "11",
        "12"
      ]
    }
  ],
  "requirements": [
    {
      "name": "openvox",
      "version_requirement": ">= 7.16 < 9.0.0"
    }
  ]
}
